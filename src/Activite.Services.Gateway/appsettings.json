{
    "app": {
        "name": "Activite.Services.Gateway",
        "displayBanner": true
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://localhost:5000"
            }
        }
    },
    "httpClient": {
        "type": "consul",
        "retries": 2,
        "services": {}
    },
    "consul": {
        "enabled": true,
        "url": "http://localhost:8500",
        "service": "activite-gateway",
        "address": "host.docker.internal",
        "port": "5000",
        "pingEnabled": true,
        "pingEndpoint": "ping",
        "pingInterval": 5,
        "removeAfterInterval": 15
    },
    "logger": {
        "level": "information",
        "excludePaths": [
        ],
        "excludeProperties": [
            "api_key",
            "access_key",
            "ApiKey",
            "ApiSecret",
            "ClientId",
            "ClientSecret",
            "ConnectionString",
            "Password",
            "Email",
            "Login",
            "Secret",
            "Token"
        ],
        "console": {
            "enabled": true
        },
        "seq": {
            "enabled": true,
            "url": "http://host.docker.internal:5341",
            "apiKey": "secret"
        },
        "tags": {}
    }
}